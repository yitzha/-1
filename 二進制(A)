#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    int a[32];
    int i;
    int x;
    scanf("%d",&x );
    i= 0 ;
    while(x)
    {
        a[i]=x%2;
        i++;
        x=x/2;
        //printf("process %d\n",a[i]);
    }
    i=i-1;
    while(i>=0){
            printf("%d", a[i]);
            i--;
    }
    return 0;
}
